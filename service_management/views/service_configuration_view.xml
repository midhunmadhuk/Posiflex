    	<!--Serial Master  -->
    	<record id="serial_no_master_form" model="ir.ui.view">
	        <field name="name">serial.no.master.form</field>
	        <field name="model">serial.no.master</field>
	        <field name="arch" type="xml">
	            <form string="Serial Master">
	            	<sheet>
	                    <group>
	                        <group>
	                        	<field name="name"/>
	                        	<field name="war_latest"/>
	                        	<field name="avail_entity_type"/>
	                        	<field name="war_start_dt"/>
	                        	<field name="war_balance"/>
	                        </group>
	                        <group>
	                        	<field name="product_id"/>
	                        	<field name="war_in_months"/>
	                        	<field name="partner_id" attrs="{'invisible': [('avail_entity_type', '!=', 'partner')]}"/>
	                        	<field name="customer_id" attrs="{'invisible': [('avail_entity_type', '!=', 'customer')]}"/>
	                        	<field name="war_end_dt"/>
	                        	<field name="war_status"/>
	                        </group>
	                    </group>
	                </sheet>
	            </form>
	        </field>
